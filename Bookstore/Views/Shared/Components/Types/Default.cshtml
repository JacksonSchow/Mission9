@{
}

@model IEnumerable<string>

<a asp-controller="Home" asp-action="Index" asp-route-Category="" asp-route-pageNum="1" class="btn btn-block btn-outline-secondary">Home</a>

@foreach (string t in Model)
{
    <a asp-controller="Home" asp-action="Index" asp-route-pageNum="1" asp-route-Category="@t"
       class="btn btn-block @(ViewBag.SelectedType == t ? "btn-info" : "btn-outline-secondary")">@t</a>
}