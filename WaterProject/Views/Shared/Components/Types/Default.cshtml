@{
}

@model IEnumerable<string>

<a asp-controller="Home" asp-action="Index" asp-route-projectType="" asp-route-pageNum="1" class="btn btn-block btn-outline-secondary">Home</a>

@foreach (string t in Model)
{
    <a asp-controller="Home" asp-action="Index" asp-route-projectType="@t" asp-route-pageNum="1" class="btn btn-block @(ViewBag.SelectedType == t ? "btn-primary" : "btn-outline-secondary")">@t</a>
}
